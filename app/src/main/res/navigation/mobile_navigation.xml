<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_map">

    <fragment
        android:id="@+id/navigation_map"
        android:name="com.example.mygeolocation.ui.home.MapFragment"
        android:label="@string/title_map"
        tools:layout="@layout/fragment_map" />

    <fragment
        android:id="@+id/navigation_list_marker"
        android:name="com.example.mygeolocation.ui.dashboard.ListMarkerFragment"
        android:label="@string/title_list_marker"
        tools:layout="@layout/fragment_list_marker" />

    <fragment
        android:id="@+id/navigation_not_use"
        android:name="com.example.mygeolocation.ui.notifications.NotUseFragment"
        android:label="@string/title_not_use"
        tools:layout="@layout/fragment_not_use" />
</navigation>